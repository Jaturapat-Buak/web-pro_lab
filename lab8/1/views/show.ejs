<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users Table</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
        }
        th, td {
            border: 1px solid black;
            padding: 8px;
            text-align: center;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>

    <h2>User Data</h2>

    <table>
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Password</th>
            <th>Email</th>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Age</th>
            <th>Address</th>
            <th>Phone</th>
        </tr>

        <% data.forEach((item) => { %>
        <tr>
            <td><%= item.id %></td>
            <td><%= item.username %></td>
            <td><%= item.password %></td>
            <td><%= item.email %></td>
            <td><%= item.firstname %></td>
            <td><%= item.lastname %></td>
            <td><%= item.age %></td>
            <td><%= item.address %></td>
            <td><%= item.phone %></td>
        </tr>
        <% }) %>

    </table>

</body>
</html>